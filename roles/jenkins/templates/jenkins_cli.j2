#!/usr/bin/env bash
JENKINS_CLI_JAR={{ jenkins_cli_jar }}
export JENKINS_URL=http://{{ jenkins_hostname }}:{{ jenkins_http_port }}{{ jenkins_url_prefix }}
{% if jenkins_cli_user is defined %}
java -jar ${JENKINS_CLI_JAR} "$@" --username="{{ jenkins_cli_user }}" --password "{{ jenkins_cli_pass }}" 
{% else %}
java -jar ${JENKINS_CLI_JAR} "$@"
{% endif %}
